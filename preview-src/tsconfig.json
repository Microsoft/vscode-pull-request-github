{
	"compilerOptions": {
		"outDir": "./dist/",
		"module": "commonjs",
		"target": "es6",
		"jsx": "react",
		"jsxFactory": "h",
		"sourceMap": true,
		"strict": true,
		"noImplicitAny": true,
		"noUnusedLocals": true,
		"baseUrl": "..",
		"paths": {
			"~/*": ["./*"]
		}
	},
	"include": [
		"./**/*"
	]
}